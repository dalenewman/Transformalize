<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <RootNamespace>Transformalize.Logging.NLog</RootNamespace>
     <AssemblyName>Transformalize.Logging.NLog</AssemblyName>
     <Version>0.11.7</Version>
     <FileVersion>0.11.7</FileVersion>
     <AssemblyVersion>0.11.7</AssemblyVersion> 
  </PropertyGroup>

  <ItemGroup>
    <None Remove="NLog.config" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="NLog.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NLog" Version="6.0.7" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Transformalize\Transformalize.csproj" />
  </ItemGroup>

</Project>
